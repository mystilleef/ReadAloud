{
  "name": "readaloud",
  "author": "Lateef Alabi-Oki <mystilleef@gmail.com>",
  "license": "gpl3",
  "version": "1.0.0",
  "description": "An extension to read aloud selected text in Google Chrome",
  "scripts": {
    "prebuild": "mkdir -p dist/js",
    "build": "rollup -c",
    "postbuild": "cp -r out/* dist/js",
    "predist": "npm run build",
    "dist": "cp -r public/* dist/",
    "clean": "rm -rf build out dist",
    "clean:build": "rm -rf out",
    "clean:out": "npm run clean:build",
    "prewatch": "npm run clean ; npm run build",
    "watch": "rollup -cw"
  },
  "keywords": [],
  "devDependencies": {
    "@types/chrome": "0.0.88",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-functional": "^1.0.0-rc.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "redux-devtools": "^3.5.0",
    "rollup": "^1.19.4",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript2": "^0.22.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "redux": "^4.0.4"
  }
}
